namespace flatbuffers_types;

table Namespace {
  name: string;  // empty means root namespace.

  services: [Service];
  objects: [Object];
  enums: [Enum];
}

DeclarationLocation {
  file: int;  // Index in file table.
  line_number: int;
  line_position: int;
}

table Generation {
  files: [string];
  root_namespace: Namespace;
}
